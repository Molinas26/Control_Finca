<div>
    <%- include("../encabezado"); %>
</div>
<center><h3>Programacion de actividades por empleado del <%= fecha%></h3></center>
<div>
    <table id="datatableTest"  class="table table-striped table-bordered">
        <thead class="bg-primary text-white" style="text-align: center;">
            <tr>
                <th>Empleado</th>
                <th>Area</th>
                <th>Sub Area</th>
                <th>Fecha</th>
            </tr>
        </thead>
        <tbody>
            <% programa.forEach(datos=>{ %>
                <tr>
                    <td><%= datos.empleado_nombre%></td>
                    <td><%= datos.nombre_area%></td>
                    <td><%= datos.nombre_subarea%></td>
                    <td><%= fecha%></td>
                </tr>
            <% }) %>
        </tbody>
    </table>
    <a href="/programacion" class="btn btn-secondary">Regresar</a>
</div>